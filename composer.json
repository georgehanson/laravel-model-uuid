{
    "name": "george/laravel-model-uuid",
    "description": "A simple package to enable the use of uuids for models",
    "license": "MIT",
    "authors": [
        {
            "name": "George Hanson",
            "email": "george@georgehanson.co.uk"
        }
    ],
    "require": {
        "illuminate/support": "^5.8|^5.7|^5.6|^5.5|^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "squizlabs/php_codesniffer": "^3.1",
        "fzaninotto/faker": "^1.7",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "GeorgeHanson\\LaravelModelUuid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GeorgeHanson\\LaravelModelUuid\\Tests\\": "tests/"
        }
    }
}
